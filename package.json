{
  "name": "node-mariadb-example",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "yarn build:tsoa && ts-node src/server.ts",
    "start:dev": "nodemon",
    "build:routes": "yarn tsoa routes",
    "build:swagger": "yarn tsoa spec",
    "build:tsoa": "yarn tsoa spec-and-routes",
    "build": "npm run build:routes && tsc && tsc-alias",
    "test": "jest",
    "typeorm": "typeorm-ts-node-esm",
    "clean": "rimraf dist",
    "migration:run": "typeorm-ts-node-esm migration:run -d src/migrations.config.ts",
    "migration:revert": "typeorm-ts-node-esm migration:revert -d src/migrations.config.ts",
    "migration:create": "typeorm-ts-node-esm migration:create"
  },
  "dependencies": {
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.18",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "express": "^4.18.2",
    "inversify": "^6.0.1",
    "jest": "^29.3.1",
    "lightship": "^7.1.1",
    "mariadb": "^3.0.2",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.20",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.2",
    "tsoa": "^4.1.3",
    "typeorm": "^0.3.11",
    "typescript": "^4.9.4",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/jest-when": "^3.5.2",
    "@types/morgan": "^1.9.3",
    "@types/yamljs": "^0.2.31",
    "jest-when": "^3.5.2",
    "ts-node": "^10.9.1"
  }
}
